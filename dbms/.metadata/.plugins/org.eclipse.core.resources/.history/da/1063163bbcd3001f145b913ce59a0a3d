SELECT sysdate FROM dual;

CREATE TABLE tbl_user(
	user_number NUMBER,
	user_id varchar2(10),
	user_pw varchar2(20),
	user_name varchar(100),
	user_age NUMBER,
	user_gender char(1),
	CONSTRAINT pk_user PRIMARY KEY(user_number)
);

CREATE SEQUENCE seq_user;

SELECT * FROM tbl_user;
DELETE FROM TBL_USER WHERE USER_NUMBER = 2;



